<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<display-name>ContactNetworkingWebApp</display-name>
	<welcome-file-list>
		<welcome-file>home.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.hsbc.networking.controller.Validation.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>RegistrationServlet</display-name>
		<servlet-name>RegistrationServlet</servlet-name>
		<servlet-class>com.hsbc.networking.controller.Validation.RegistrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/RegistrationServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>UserInfo</display-name>
		<servlet-name>UserInfo</servlet-name>
		<servlet-class>com.hsbc.networking.controller.UserServlet.UserInfo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserInfo</servlet-name>
		<url-pattern>/UserInfo</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ViewContactServlet</display-name>
		<servlet-name>ViewContactServlet</servlet-name>
		<servlet-class>com.hsbc.networking.controller.ContactServlet.ViewContactServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewContactServlet</servlet-name>
		<url-pattern>/ViewContactServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>AdminServlet</display-name>
		<servlet-name>AdminServlet</servlet-name>
		<servlet-class>com.hsbc.networking.controller.AdminServlet.AdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminServlet</servlet-name>
		<url-pattern>/AdminServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>AdminLogin</display-name>
		<servlet-name>AdminLogin</servlet-name>
		<servlet-class>com.hsbc.networking.controller.AdminServlet.AdminLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminLogin</servlet-name>
		<url-pattern>/AdminLogin</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>AdminLogOutServlet</display-name>
		<servlet-name>AdminLogOutServlet</servlet-name>
		<servlet-class>com.hsbc.networking.controller.AdminServlet.AdminLogOutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminLogOutServlet</servlet-name>
		<url-pattern>/AdminLogOutServlet</url-pattern>
	</servlet-mapping>
</web-app>